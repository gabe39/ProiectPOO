<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/14.0.2.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Button layoutX="284.0" layoutY="14.0" mnemonicParsing="false" onAction="#up" text="Up">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font></Button>
        <Button layoutX="535.0" layoutY="188.0" mnemonicParsing="false" onAction="#right" text="Right">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font></Button>
        <Button layoutX="274.0" layoutY="361.0" mnemonicParsing="false" onAction="#down" text="Down">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font>
        </Button>
        <Button layoutX="14.0" layoutY="201.0" mnemonicParsing="false" onAction="#left" text="Left">
            <font>
                <Font name="Arial Black" size="12.0" />
            </font>
        </Button>
        <Circle fx:id="circ" fill="RED" layoutX="300.0" layoutY="213.0" radius="33.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="4.0" />
      <Button layoutX="535.0" layoutY="361.0" mnemonicParsing="false" onAction="#SwitchToPage2" text="Page 2" />
    </children>


</AnchorPane>
