public class Alarma{
    private String ora,ziua,numeAlarma;
    private double sold;

    public Alarma(String O,String Z,String NA)
    {this.ora=O;
    this.ziua=Z;
    this.numeAlarma=NA;
    }
    
public String getOra()
{return this.ora;}
public String getZiua()
{return this.ziua;}
public String getNumeAlarma()
{return this.numeAlarma;}